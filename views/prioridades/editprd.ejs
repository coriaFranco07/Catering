
<%- include ('../partials/head.ejs') %>
      <% if (prioridad) { %>
        <div>
          <a href="/prds" class="btn btn-block btn-outline-secondary btn-xs align-items-right"><i class="fas fa-undo"></i></a>
        </div>
      <div class="container mt-4">
      <div class="card border-secondary">
        <div class="card-header">Editar Registro</div>
            <div class="card-body">
                <form action="../update" method="POST">
                    <input id="id_prioridad" name="id_prioridad" value="<%= prioridad.id_prioridad%>" hidden>
                    
                    <div class="mb-3">
                      <label for="dsc_prioridad" class="form-label">Elemento</label>
                      <input type="text" class="form-control" id="dsc_prioridad" name="dsc_prioridad" value="<%= prioridad.dsc_prioridad %>" tabindex="1">
                  </div>
                 
                
                    <button type="submit" class="btn btn-primary" tabindex="5">Guardar</button>
                    <a href="/prds" class="btn btn-secondary">Cancelar</a>

                </form>
            </div>
        </div>
      </div>
<% } %>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>

  
  </body>
</html>