<%- include ('../partials/head.ejs') %>
      <div class=" container mt-4 col-12 d-flex">
        <% if (typeof validaciones != 'undefined'){ %>
          <% validaciones.forEach((valid)=>{%>

            <strong class="btn btn-outline-danger btn-sm"><%=valid%></strong>

             
          <%})%>

        <% } %>
      </div>
      
      <div class="container mt-4 col-12 d-flex">
        
      <div class="card border-secondary">
        <div class="card-header bg-primary text-white text-center">NUEVO REGISTRO</div>
            <div class="card-body">
              
                <form action="/exptes/procesos/add" method="post">
                  
                  
                  <div class="row mt-3">

                    <div class="col-12">
                      <input type="hidden"  id="id_expte" name="id_expte"  value="<%= expte %>">
                      <span class="input-group-text d-sm-block bg-primary text-white">Elementos</span>
                      <select id="id_tipo_elemento" name="id_tipo_elemento" class="form-select " tabindex="3" >
                        <% elementos.forEach((proc) => { %>
                          <option class="text-sm-center" value="<%= proc.id_tipo_elemento %>"><%=proc.dsc_tipo_elemento %></option>
                         <% }) %>
                      </select>
                    </div>
                    
                 
                </div>
                  
                 <div class="row-cols-sm-8 mt-3">                   
                    <button type="submit" class="btn btn-primary" tabindex="4">Guardar</button>
                    <a href="/exptes/procesos/<%= expte %>" class="btn btn-secondary">Cancelar</a>
                  </div>
                </form>
            </div>
        </div>
      </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <% if(alert) {%>
    
      <script>
        
        Swal.fire({
          title: '<%= alertTitle  %>',
          text: '<%= AlertMessage  %>',
          icon: '<%= alertIcon  %>',
          showConfirmButton: '<%= showConfirmButton  %>',
          timer: '<%= timer  %>'
        })
        /* .then(()=>{
          window.location= '<%= ruta %>'
          window.location= '/exptes/create'
        }) */
    
    </script>
      
    
    
    <% } %>  

  
  </body>
</html>