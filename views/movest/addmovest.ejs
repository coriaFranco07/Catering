<%- include ('../partials/head.ejs') %>
<div class=" container mt-4 col-12 d-flex">
  <% if (typeof validaciones != 'undefined'){ %>
    <% validaciones.forEach((valid)=>{%>

      <strong class="btn btn-outline-danger btn-sm"><%=valid%></strong>

       
    <%})%>

  <% } %>
</div>
      <div class="container mt-4">
      <div class="card border-secondary">
        <div class="card-header">Nuevo Registro</div>
            <div class="card-body">
                <form action="/ests/movests/addmovest" method="post">
                    <div class="mb-3">
                        <label for="est" class="form-label">ESTADO MOVIMIENTOS</label>
                        <input type="text" class="form-control" id="movest" name="movest" tabindex="1">
                    </div>
                    
                    
                    <button type="submit" class="btn btn-primary" tabindex="5">Guardar</button>
                    <a href="/ests/movests" class="btn btn-secondary">Cancelar</a>

                </form>
            </div>
        </div>
      </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
    <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <% if(alert) {%>
    
      <script>
        
        Swal.fire({
          title: '<%= alertTitle  %>',
          text: '<%= AlertMessage  %>',
          icon: '<%= alertIcon  %>',
          showConfirmButton: '<%= showConfirmButton  %>',
          timer: '<%= timer  %>'
        }).then(()=>{
          window.location= '<%= ruta %>'
        })
    
    </script>
      
    
    
    <% } %>  

  
  </body>
</html>